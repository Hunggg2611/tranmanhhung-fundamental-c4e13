prices = {
    "banana": 4,
    "apple": 2,
    "orange": 1.5,
    "pear": 3,
}
stock = {
    "banana": 6,
    "apple": 0,
    "orange": 32,
    "pear": 15,
}
total = 0
for t in stock.keys():
    print(t, stock[t])
    print("prices:",prices[t])
    eq = stock[t]*prices[t]
    print()
    total += eq
print("Total ",total)
